    <fields>
        <fullName>QR_Code__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Campaign__r.Name == &apos;Bean Game&apos;,(IF( ISPICKVAL(Contact_Id__r.Language__c,&apos;English&apos;), $Label.Community_Url + &quot;/s/add-beangame?participantcode=&quot; +  Contact_Id__r.ParticipantCode__c, $Label.Community_Url_Spanish + &quot;/s/adic/juegofrijol?participantcode=&quot; +  Contact_Id__r.ParticipantCode__c)),&apos;&apos;)</formula>
