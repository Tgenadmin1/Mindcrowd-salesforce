        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Profile_Name__c</fullName>
        <description>Community user profile needed for user creation in automated process.</description>
        <externalId>false</externalId>
        <label>Profile Name</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
